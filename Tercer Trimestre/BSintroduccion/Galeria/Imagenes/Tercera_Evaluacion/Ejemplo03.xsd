<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="desayunos">
    <xs:complexType>
        <xs:sequence>
            <xs:element name="comida" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                <xs:sequence>
                    <xs:element name="nombre" type="xs:string"></xs:element>
                    <xs:element name="precio" type="xs:string"></xs:element>
                    <xs:element name="descripcion" type="xs:string"></xs:element>
                    <xs:element name="calorias" type="xs:string"></xs:element>
                </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    </xs:element>
    </xs:schema>